(define (problem turn_on_every_light)
  (:domain shakey)
  (:objects rooma roomb roomc switcha switchb switchc boxa wideab widebc narrowbc left right)
  (:init (room rooma)
          (room roomb)
	  (room roomc)
	  (door wideab)
	  (door widebc)
	  (door narrowbc)
	  (lightswitch switcha)
	  (lightswitch switchb)
	  (lightswitch switchc)
	  (box boxa)
          (gripper left)
          (gripper right)
          (at-shakey rooma)
          (free left)
          (free right)
	  (wide wideab)
	  (wide widebc)
	  (door-between wideab rooma roomb)
	  (door-between widebc roomb roomc)
	  (door-between narrowbc roomb roomc)
	  (at boxa rooma)
	  (at switcha rooma)
          (at switchb roomb)
	  (at switchc roomc)
	  (lightswitch-on switchb)
	  )
   (:goal (and (lightswitch-on switha) (lightswitch-on swithb) (lightswitch-on swithc))